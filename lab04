        cout << "Enter CGPA:" <<endl;

         for (int i=0; i<10; i++){
        cin >> arr[i];}

    for(int i=0; i<10;i++){
        cout<<arr[i]<<" ";}

    cout<<endl<<"---------"<<endl;

    for(int i=0; i<10; i++){
        min_index = i;

        for(int j=i+1; j<10; j++){
            if(arr[min_index] > arr[j]){
                min_index = j;}
        }

        temp = arr[min_index];
        arr[min_index] = arr[i];
        arr[i] = temp;
    }

    for(int i=0; i<10;i++){
        cout<<arr[i]<<" ";}

    cout<<endl;

    return 0;
}





