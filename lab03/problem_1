#include<iostream>

using namespace std;

int main(){

    int i,j,n,m;

    cout << "Enter the row size:";
    cin >>n;
    cout << "Enter the column size:";
    cin >>m;

    int arr[n][m];

    cout << "Enter the element " ;
    for (int i=0; i<n; i++){
        for(int j=0;j<m;j++) {
            cin >> arr[i][j];
        }
    }
    cout << "Matrix:"<< endl;
    for (int i=0; i<n; i++){
        for(int j=0; j<m; j++){
            cout << arr[i][j] << " ";
        }
        cout << endl;
    }
    return 0;
}
